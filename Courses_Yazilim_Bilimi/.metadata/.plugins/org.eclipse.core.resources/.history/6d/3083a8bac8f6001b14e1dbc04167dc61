#include <iostream>
using namespace std;

class Employee
{
private:
	string name;

public:

	void setAge(int yas)
	{
		if (yas < 0)
		{
			cout << "Böyle bir değer veremezsiniz." << endl;
		}

		else
		{
			age = yas;
		}
	}

	int getAge()
	{
		return age;
	}

	void setName(string isim)
	{
		name = isim;
	}

	string getName()
	{
		return name;
	}
};


int main() {



	return 0;
}

