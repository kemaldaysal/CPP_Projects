#include <iostream>
using namespace std;

class Student
{
	public:
	string name; // özellik

	void tellName() // metod // classa method eklemiş olduk.
	{
		cout << "Benim adım: " << name << endl;
	}

};

int main() {

	Student student1;

	student1.name = "Kemal Daysal";

	student1.tellName();


	return 0;
}
