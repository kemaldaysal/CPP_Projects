#include <iostream>
using namespace std;

class Employee
{
private:
	string name;
	int age;
	int salary;

public:
	Employee(string name, int age, int salary)
	{
		this->name = name;
		this->age = age;
		this->salary = salary;
	}
};

class Test
{
public:
	static void showInfos(Employee employee)
	{
	cout << employee.name << " " << employee.age << " " << employee.salary << endl;
	}
};



int main() {



	return 0;
}

